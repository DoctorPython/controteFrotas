{
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/public/**"
    }
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "public": false
}
